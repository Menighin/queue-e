extends layout

block content

    div.index-page
        .processes-container
            each p in processes
                .process
                    .title= p.name
                    .download-log 
                        a(href=`/process/downloadLog?id=${p.id}&name=${p.name}`) LOG
                    
    
    script.
        let socket = io('/queue-progress');
        socket.on('update', (data) => {
            document.getElementById('test').innerHTML += data + '<br>';
        });
    
