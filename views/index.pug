extends layout

block content

    div.index-page
        h1= title
        p Welcome to #{title}. It works
        ul
            each p in processes
                li= JSON.stringify(p)

    
    script.
        let socket = io('/queue-progress');
        socket.on('update', (data) => {
            document.getElementById('test').innerHTML += data + '<br>';
        });
    
